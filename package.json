{
  "private": false,
  "scripts": {
    "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged && node ./.script/genBlockConfig.js&& git add ./umi-block.json",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty  ./ && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./",
    "lint:prettier": "prettier --check \"**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"**/*.less\" --syntax less",
    "prettier": "prettier -c --write \"**/*\"",
    "screenshot": "pro screenshot",
    "start": "umi dev",
    "test": "jest",
    "doc": "typedoc --options ./typedoc.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": ""
    }
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write",
      "git add"
    ],
    "**/*.{js,jsx}": "npm run lint-staged:js",
    "**/*.{js,ts,tsx}": "npm run lint-staged:js"
  },
  "dependencies": {
    "@ant-design/icons": "^4.0.0",
    "@ant-design/pro-cli": "^1.0.5",
    "@ant-design/pro-layout": "^5.0.0",
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
    "@testing-library/react": "^10.4.8",
    "@types/jsdom": "^16.2.3",
    "@types/react-test-renderer": "^16.9.3",
    "antd": "^4.0.0",
    "babel-jest": "^26.2.2",
    "bizcharts": "^3.5.4",
    "jsdom": "^16.4.0",
    "numeral": "^2.0.6",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "typedoc": "^0.17.8",
    "umi": "^3.2.16",
    "umi-request": "^1.0.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.10.4",
    "@types/classnames": "^2.2.7",
    "@types/enzyme": "^3.10.5",
    "@types/jest": "^26.0.9",
    "@types/numeral": "^0.0.28",
    "@types/react-dom": "^16.8.4",
    "@umijs/fabric": "^2.0.0",
    "@umijs/plugin-block-devtool": "^1.1.1",
    "@umijs/preset-ant-design-pro": "^1.1.2",
    "@umijs/preset-react": "^1.4.3",
    "cross-env": "^7.0.0",
    "css-loader": "^4.2.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.5.0",
    "eslint": "^7.2.0",
    "husky": "^4.0.1",
    "less": "^3.12.2",
    "less-loader": "^7.0.0",
    "lint-staged": "^10.0.1",
    "mockdate": "^3.0.2",
    "mockjs": "^1.1.0",
    "prettier": "^2.0.2",
    "style-loader": "^1.2.1",
    "stylelint": "^13.0.0",
    "typescript": "^3.5.1"
  },
  "checkFiles": [
    "**/*.ts*",
    "**/*.less",
    "**/*.md"
  ]
}
